<h1>Login</h1>

<h2><%= flash[:error] %></h2>

<%= form_tag(contractor_login_path, method: 'post') do %>
  <p>
    <%= label_tag(:email, "Email") %>
    <%= text_field_tag(:email) %>
  </p>

  <p>
    <%= label_tag(:password, "Password") %>
    <%= password_field_tag(:password) %>
  </p>

  <p>
    <%= submit_tag("Submit") %>
  </p>
<% end %>
